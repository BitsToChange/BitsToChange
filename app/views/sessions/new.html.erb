<h1>Log In</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password, params[:password] %>
  </p>
  <p class="button"><%= button_tag 'Log In' %></p>
<% end %>